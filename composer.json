{
	"name": "janpecha/code-checker",
	"type": "project",
	"description": "Code checker.",
	"license": "BSD-3-Clause",
	"authors": [
		{
			"name": "Jan Pecha",
			"homepage": "https://www.janpecha.cz/"
		}
	],
	"require": {
		"php": "8.2 - 8.4",
		"czproject/assert": "^2.0",
		"czproject/path-helper": "^4.1",
		"czproject/git-php": "^4.0",
		"czproject/php-simple-ast": "^1.5",
		"symfony/yaml": "^7.0",
		"nette/utils": "^4.0",
		"composer/semver": "^3.4",
		"nette/command-line": "^1.8",
		"nette/neon": "^3.4"
	},
	"require-dev": {
		"nette/tester": "^2.5"
	},
	"autoload": {
		"classmap": ["src/"]
	},
	"extra": {
		"phpstan-extensions": [
			"phpstan/phpstan-nette"
		]
	}
}
