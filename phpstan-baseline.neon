parameters:
	ignoreErrors:
		-
			message: '#^Call to function is_callable\(\) with array\{class\-string, ''configure''\} will always evaluate to false\.$#'
			identifier: function.impossibleType
			count: 1
			path: src/CheckerFactory.php

		-
			message: '#^Parameter \#1 \$parameters of method JP\\CodeChecker\\CheckerConfig\:\:setParameters\(\) expects array\<string, mixed\>, array\<mixed, mixed\> given\.$#'
			identifier: argument.type
			count: 1
			path: src/CheckerFactory.php

		-
			message: '#^Unreachable statement \- code above always terminates\.$#'
			identifier: deadCode.unreachable
			count: 1
			path: src/CheckerFactory.php

		-
			message: '#^Method JP\\CodeChecker\\ComposerFile\:\:getLicense\(\) should return array\<string\> but returns array\<mixed\>\.$#'
			identifier: return.type
			count: 1
			path: src/ComposerFile.php

		-
			message: '#^Method JP\\CodeChecker\\ComposerFile\:\:getRequire\(\) should return array\<string, string\> but returns array\<mixed, mixed\>\.$#'
			identifier: return.type
			count: 1
			path: src/ComposerFile.php

		-
			message: '#^Method JP\\CodeChecker\\ComposerFile\:\:getRequireDev\(\) should return array\<string, string\> but returns array\<mixed, mixed\>\.$#'
			identifier: return.type
			count: 1
			path: src/ComposerFile.php

		-
			message: '#^Parameter \#2 \$data of class JP\\CodeChecker\\ComposerFile constructor expects array\<string, mixed\>, array\<mixed, mixed\> given\.$#'
			identifier: argument.type
			count: 1
			path: src/ComposerFile.php

		-
			message: '#^Method JP\\CodeChecker\\Engine\:\:findFiles\(\) return type with generic class AppendIterator does not specify its types\: TKey, TValue, TIterator$#'
			identifier: missingType.generics
			count: 1
			path: src/Engine.php

		-
			message: '#^Method JP\\CodeChecker\\Engine\:\:findFiles\(\) should return AppendIterator&iterable\<SplFileInfo\> but returns AppendIterator\<mixed, mixed, Iterator\<TKey, TValue\>\>\.$#'
			identifier: return.type
			count: 1
			path: src/Engine.php

		-
			message: '#^Method JP\\CodeChecker\\Engine\:\:findScannedFiles\(\) return type with generic class AppendIterator does not specify its types\: TKey, TValue, TIterator$#'
			identifier: missingType.generics
			count: 1
			path: src/Engine.php

		-
			message: '#^Method JP\\CodeChecker\\Engine\:\:findScannedFiles\(\) should return AppendIterator&iterable\<SplFileInfo\> but returns AppendIterator\<mixed, mixed, Iterator\<TKey, TValue\>\>\.$#'
			identifier: return.type
			count: 1
			path: src/Engine.php

		-
			message: '#^Cannot access offset ''phpVersions'' on mixed\.$#'
			identifier: offsetAccess.nonOffsetAccessible
			count: 3
			path: src/Extensions/JanpechaActionsExtension.php

		-
			message: '#^Cannot access offset ''with'' on mixed\.$#'
			identifier: offsetAccess.nonOffsetAccessible
			count: 4
			path: src/Extensions/JanpechaActionsExtension.php

		-
			message: '#^Cannot access offset ''workingDirectory'' on mixed\.$#'
			identifier: offsetAccess.nonOffsetAccessible
			count: 1
			path: src/Extensions/JanpechaActionsExtension.php

		-
			message: '#^Parameter \#1 \$value of function count expects array\|Countable, mixed given\.$#'
			identifier: argument.type
			count: 2
			path: src/Extensions/JanpechaActionsExtension.php

		-
			message: '#^Parameter \#1 \$yaml of method JP\\CodeChecker\\Extensions\\JanpechaActionsExtension\:\:createTestsJob\(\) expects array\<string, mixed\>, array given\.$#'
			identifier: argument.type
			count: 1
			path: src/Extensions/JanpechaActionsExtension.php
